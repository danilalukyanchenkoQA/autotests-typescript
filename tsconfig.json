{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom", "ES2015"],
    "types": ["cypress", "node"],
    "module": "Node16", // Должно соответствовать moduleResolution
    "moduleResolution": "node16",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}